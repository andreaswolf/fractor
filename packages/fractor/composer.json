{
    "name": "a9f/fractor",
    "description": "File Read-Analyse-Change TOol",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andreas Wolf",
            "email": "dev@a-w.io",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "nette/utils": "^4.0",
        "sebastian/diff": "^5.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/filesystem": "^6.4 || ^7.0",
        "symfony/finder": "^6.4 || ^7.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "symplify/rule-doc-generator-contracts": "^11.2"
    },
    "autoload": {
        "psr-4": {
            "a9f\\Fractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "a9f\\Fractor\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/fractor"
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2-dev"
        },
        "split-repository": "fractor-fractor"
    }
}
