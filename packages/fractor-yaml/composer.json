{
    "name": "a9f/fractor-yaml",
    "description": "YAML extension for the File Read-Analyse-Change TOol. Allows modifying YAML files",
    "license": "MIT",
    "type": "fractor-extension",
    "authors": [
        {
            "name": "Andreas Wolf",
            "email": "dev@a-w.io",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "a9f/fractor": "^0.2",
        "a9f/fractor-extension-installer": "^0.2",
        "symfony/yaml": "^6.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "symplify/rule-doc-generator-contracts": "^11.2"
    },
    "repositories": {
        "fractor": {
            "type": "path",
            "url": "../*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "a9f\\FractorYaml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "a9f\\FractorYaml\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "a9f/fractor-extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2-dev"
        }
    }
}
