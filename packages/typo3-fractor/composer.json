{
    "name": "a9f/typo3-fractor",
    "description": "TYPO3 extension for the File Read-Analyse-Change Tool. Allows modifying XML files",
    "license": "MIT",
    "type": "fractor-extension",
    "authors": [
        {
            "name": "Andreas Wolf",
            "email": "dev@a-w.io",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-dom": "*",
        "a9f/fractor": "^0.5",
        "a9f/fractor-extension-installer": "^0.5",
        "a9f/fractor-fluid": "^0.5",
        "a9f/fractor-typoscript": "^0.5",
        "a9f/fractor-xml": "^0.5",
        "a9f/fractor-yaml": "^0.5",
        "symplify/rule-doc-generator-contracts": "^11.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "a9f\\Typo3Fractor\\": [
                "src/",
                "rules/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "a9f\\Typo3Fractor\\Tests\\": [
                "tests/",
                "rules-tests/"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "a9f/fractor-extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.5-dev"
        }
    }
}
