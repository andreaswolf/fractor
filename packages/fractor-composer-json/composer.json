{
    "name": "a9f/fractor-composer-json",
    "description": "ComposerJson extension for the File Read-Analyse-Change Tool. Allows modifying composer.json files",
    "license": "MIT",
    "type": "fractor-extension",
    "authors": [
        {
            "name": "Andreas Wolf",
            "email": "dev@a-w.io",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "a9f/fractor": "^0.2",
        "a9f/fractor-doc-generator": "^0.2",
        "a9f/fractor-extension-installer": "^0.2",
        "ergebnis/json-printer": "^3.5",
        "eta-orionis/composer-json-manipulator": "^1.0.1",
        "nette/utils": "^4.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "symplify/rule-doc-generator": "12.1.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "a9f\\FractorComposerJson\\": [
                "src",
                "rules"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "a9f\\FractorComposerJson\\Tests\\": [
                "tests",
                "rules-tests"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "a9f/fractor-extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2-dev"
        }
    }
}
